{
    "manifest_version": 3,
    "name": "IGN MaCarte Utilities",
    "version": "1.0",

    "description": "Ajoute des fonctionnalités à macarte.ign.fr",

    "icons": {},

    "content_scripts": [
      {
        "matches": ["https://macarte.ign.fr/*"],
        "js": [
          "main.js"
        ]
      }
    ],
    "web_accessible_resources": [
      {
        "matches": ["https://macarte.ign.fr/*"],
        "resources": [
          "ImuGui.js",
          "ImuActions.js",
          "ImuUtils.js",
          "ImuInit.js"
        ]
      }
    ]
  }